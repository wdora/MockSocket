{
  "MockSocket": {
    "HeartInterval": 30,
    "RemoteServer": {
      "Host": "wdora.com",
      "Port": 9090
    },
    "RealServer": {
      "Host": "127.0.0.1",
      "Port":3389
    },
    "AppServer": {
      "Port": 33890
    }
  },
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "async": false,
      "logfile": {
        "type": "File",
        "fileName": "logs/nlog-${shortdate}.log",
        "layout": "${time}|${uppercase:${level}}|${message} ${exception:format=toString}"
      },
      "logconsole": {
        "type": "Console",
        "layout": "${date:format=HH\\:mm\\:ss.fff} ${level:uppercase=true} ${logger:shortName=True} ${message} ${exception:format=ToString}"
      }
    },
    "rules": [
      {
        "logger": "Microsoft.*",
        "finalMinLevel": "Error"
      },
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "logconsole"
      },
      {
        "logger": "*",
        "minLevel": "Info",
        "writeTo": "logfile"
      }
    ]
  }
}